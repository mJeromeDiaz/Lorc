{
	"name": "lorc",
	"displayName": "Lorc - Symfony Terminal Command",
	"description": "List of recurrent Symfony (and env) command",
	"version": "1.0.1",
	"icon": "images/orc.png",
	"publisher": "jeromeDiaz",
	"repository": {
		"type": "git",
		"url": "https://github.com/mJeromeDiaz/Lorc.git"
	  },
	"engines": {
		"vscode": "^1.50.0"
	},
	"categories": [
		"Other"
	],
	"keywords": [
		"Terminal",
		"CMD",
		"PowerShell",
		"Command",
		"Commands",
		"Shortcuts",
		"CLI",
		"Command Line",
		"Run",
		"Symfony",
		"Composer",
		"Bundle",
		"List",
		"Orc"
	  ],
	"activationEvents": [
		"onCommand:extension.lorc"
	],
	"main": "./out/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "extension.lorc",
				"title": "List of recurrent Symfony (and env) command"
			}
		]
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./",
		"pretest": "npm run compile",
		"test": "node ./out/test/runTest.js"
	},
	"devDependencies": {
		"@types/glob": "^7.1.1",
		"@types/mocha": "^5.2.6",
		"@types/node": "^10.12.21",
		"@types/vscode": "^1.50.0",
		"glob": "^7.1.4",
		"mocha": "^6.1.4",
		"typescript": "^3.3.1",
		"tslint": "^5.12.1",
		"vscode-test": "^1.0.0-next.0"
	}
}